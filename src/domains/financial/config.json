  "deltas": {
    "strong_nrr": {
      "type": "additive",
      "value": 15,
      "description": "Net Revenue Retention (NRR) above 120%"
    },
    "weak_nrr": {
      "type": "additive",
      "value": -20,
      "description": "Net Revenue Retention (NRR) below 90% (high churn)"
    },
    "market_expansion": {
      "type": "additive",
      "value": 12,
      "description": "Active geographic or market segment expansion"
    },
    "market_contraction": {
      "type": "additive",
      "value": -15,
      "description": "Exiting markets or reducing geographic footprint"
    },
    "strong_sales_team": {
      "type": "additive",
      "value": 10,
      "description": "Recently scaled sales team (2x+ headcount growth)"
    },
    "sales_challenges": {
      "type": "additive",
      "value": -12,
      "description": "High sales team turnover or quota attainment issues"
    },
    "product_innovation": {
      "type": "additive",
      "value": 8,
      "description": "Major product launch or platform expansion imminent"
    },
    "product_stagnation": {
      "type": "additive",
      "value": -10,
      "description": "Product roadmap delays or competitive feature gap"
    },
    "economic_tailwind": {
      "type": "additive",
      "value": 6,
      "description": "Industry benefiting from macroeconomic trends"
    },
    "economic_headwind": {
      "type": "additive",
      "value": -8,
      "description": "Industry facing macroeconomic pressure"
    },
    "price_increase": {
      "type": "additive",
      "value": 5,
      "description": "Successful price increase (5-15% ARPU growth)"
    },
    "price_pressure": {
      "type": "additive",
      "value": -6,
      "description": "Pricing pressure from competition or market"
    }
  },
  "distractors": [
    "The company recently moved to a new headquarters with modern amenities.",
    "The CEO was featured in a prominent industry publication last quarter.",
    "The company has a popular employee wellness program.",
    "The founding team met at a prestigious university.",
    "The company sponsors a local sports team.",
    "The office has an award-winning interior design.",
    "The company was named one of the best places to work regionally.",
    "The CFO previously worked at a Fortune 500 company.",
    "The company has a strong social media presence.",
    "The annual company retreat is held at a luxury resort."
